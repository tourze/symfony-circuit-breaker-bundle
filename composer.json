{
  "name": "tourze/symfony-circuit-breaker-bundle",
  "description": "为 Symfony 应用提供分布式熔断器功能，支持故障隔离和服务降级",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-redis": "*",
    "doctrine/dbal": "^4.0",
    "psr/log": "^3|^2|^1",
    "symfony/cache": "^7.3",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-client": "^7.3",
    "symfony/http-client-contracts": "^3.6",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/doctrine-indexed-bundle": "1.0.*",
    "tourze/enum-extra": "1.*",
    "tourze/redis-dedicated-connection-bundle": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-routing-auto-loader-bundle": "0.0.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/easy-admin-enum-field-bundle": "1.0.*",
    "tourze/easy-admin-menu-bundle": "1.0.*",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-enum": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "suggest": {
    "ext-redis": "使用Redis存储熔断器状态",
    "easycorp/easyadmin-bundle": "为熔断器数据提供后台管理界面",
    "tourze/easy-admin-enum-field-bundle": "在EasyAdmin中支持枚举字段的中文显示和Badge样式",
    "knplabs/knp-menu": "菜单管理功能",
    "tourze/easy-admin-menu-bundle": "EasyAdmin菜单集成"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\CircuitBreaker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\CircuitBreaker\\Tests\\": "tests"
    }
  }
}